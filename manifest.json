{
  "manifest_version": 3,
  "name": "TNT - Team New Tab",
  "version": "1.0.0",
  "description": "Replace your new tab with a customizable dashboard of internal links",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
